{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "class-name": 2,
    "comment-format": [
      2,
      "check-space"
    ],
    "indent": [
      2,
      2
    ],
    "no-eval": 2,
    "no-internal-module": 2,
    "no-trailing-whitespace": 2,
    "no-unsafe-finally": 2,
    "no-var-keyword": 2,
    "one-line": [
      2,
      "check-open-brace",
      "check-whitespace"
    ],
    "semicolon": [
      2,
      "always",
      "strict-bound-class-methods"
    ],
    "triple-equals": [
      2,
      "allow-null-check"
    ],
    "typedef-whitespace": [
      2,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      2,
      "ban-keywords"
    ],
    "trailing-comma": [
      2,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "whitespace": [
      2,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "allowMultiplePropertiesPerLine": 2
  }
}
