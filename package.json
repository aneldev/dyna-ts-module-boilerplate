{
  "name": "dyna-ts-module-boilerplate",
  "version": "1.1.0",
  "description": "About",
  "repository": {
    "type": "git",
    "url": "http://github.com/aneldev/dyna-ts-module-boilerplate"
  },
  "main": "./dist/index.js",
  "preinstall": {
    ".install-tsc": "npm install -g typescript"
  },
  "bin": {
    "create-module": "./boilerplate/createModule.js",
    "create-module-with-react": "./boilerplate/createModuleWithReact.js"
  },
  "preferGlobal": "true",
  "scripts": {
    "clear": "rimraf build temp",
    "build": "rimraf build && webpack --config webpack.build.config.js",
    "debug-build": "rimraf build && webpack --config webpack.debug.nodejs.config.js --watch",
    "debug-browser": "webpack-dev-server --config webpack.debug.browser.config.js --port 8027 --devtool eval --progress --hot --content-base debug-ground/debug-on-browser --host 0.0.0.0",
    "debug-devtools": "node --inspect ./debug-ground/debug-on-nodejs/index.js  && npm run debug-devtools",
    "debug-devtools-brk": "node --inspect --debug-brk ./debug-ground/debug-on-nodejs/index.js  && npm run debug-devtools-brk",
    "debug-inspector": "node-debug ./debug-ground/debug-on-nodejs/index.js  && npm run debug-inspector",
    "debug-inspector-brk": "node-debug --debug-brk ./debug-ground/debug-on-nodejs/index.js  && npm run debug-inspector-brk",
    "debug-tsc-watch": "rimraf temp && cd debug && tsc --w",
    "run-debug-build": "node debug-ground/debug-on-nodejs/index.js",
    "release": "npm run dist",
    "dist": "rimraf ./dist/ && npm run .build-index.d.ts && npm run .build-dist",
    ".build-index.d.ts": "tsc && rm ./dist/index.js && npm run .build-index.d.ts-clear",
    ".build-index.d.ts-clear": "cd dist/ && find . -name \\*.js -delete && cd .. && rimraf temp",
    ".build-dist": "set NODE_ENV=production && webpack --config webpack.dist.config.js",
    "test": "npm run dist && jest --coverage --no-cache",
    "test-only": "jest --coverage --verbose --no-cache",
    "test-watch": "jest --watch --verbose",
    "publish-push": "npm version patch && npm publish && git push",
    "tpp": "npm run test-publish-push",
    "test-publish-push": "npm test && npm version patch && npm publish && git push",
    "reinstall": "rm -rf node_modules/ && npm install",
    "reinstall-hard": "rm package-lock.json && rm -rf node_modules/ && npm install"
  },
  "keywords": [],
  "author": "Anel dev",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4"
  },
  "devDependencies": {
    "@types/jest": "^20.0.0",
    "@types/node": "^7.0.14",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "6.11.4",
    "babel-loader": "6.0.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "6.11.6",
    "file-loader": "0.9.0",
    "font-awesome": "4.6.3",
    "jest": "^20.0.0",
    "jest-cli": "^20.0.0",
    "rimraf": "^2.6.1",
    "source-map-loader": "0.1.5",
    "ts-jest": "^20.0.0",
    "typescript": "^2.4.1",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "automock": false,
    "collectCoverage": false,
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src|tests)/.*\\.(test|spec)\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ]
  },
  "jest-junit": {
    "output": "./coverage/junit.xml"
  }
}
