{
  "name": "dyna-ts-module-boilerplate",
  "version": "1.0.0",
  "description": "About",
  "main": "./dist",
  "scripts": {
    "//help": "https://webpack.github.io/docs/webpack-dev-server.html#webpack-dev-server-cli",
    "clear": "rimraf build temp",
    "build": "rimraf build && webpack --config webpack.build.config.js",
    "debug-build": "rimraf build && webpack --config webpack.debug.nodejs.config.js --watch",
    "debug-browser": "webpack-dev-server --config webpack.debug.browser.config.js --port 8027 --devtool eval --progress --hot --content-base debug-ground/debug-on-browser --host 0.0.0.0",
    "debug-devtools": "node --inspect ./debug-ground/debug-on-nodejs/index.js  && npm run debug-devtools",
    "debug-devtools-brk": "node --inspect --debug-brk ./debug-ground/debug-on-nodejs/index.js  && npm run debug-devtools",
    "debug-inspector": "node-debug ./debug-ground/debug-on-nodejs/index.js  && npm run debug-devtools2",
    "debug-inspector-brk": "node-debug --debug-brk ./debug-ground/debug-on-nodejs/index.js  && npm run debug-devtools2",
    "debug-tsc": "rimraf temp && tsc ./debug/index.ts --outDir ./temp --w",
    "release": "npm run dist",
    "dist": "npm run .install-dts-generator && npm run .build-dist && npm run .build-index.d.ts",
    ".install-dts-generator": "if ! which dts-generator; then npm install -g dts-generator; fi",
    ".build-dist": "rimraf dist && set NODE_ENV=production && webpack --config webpack.dist.config.js",
    ".build-index.d.ts": "dts-generator --name dyna-ts-module-boilerplate --project ./ --out ./dist/index.d.ts",
    "test": "jest --coverage",
    "test-watch": "jest --watch"
  },
  "keywords": [],
  "author": "Anel dev",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-stage-2": "^6.24.1",
    "react": "^15.5.4",
    "dyna-cache": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.14",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "6.11.4",
    "babel-eslint": "5.0.0-beta4",
    "babel-loader": "6.0.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "6.11.6",
    "concurrently": "^3.4.0",
    "file-loader": "0.9.0",
    "font-awesome": "4.6.3",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "rimraf": "^2.6.1",
    "source-map-loader": "0.1.5",
    "ts-jest": "^20.0.0",
    "typescript": "^2.3.2",
    "typings": "^2.1.1",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/*\\.(test|spec))\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coverageDirectory": "temp/coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ]
  }
}
